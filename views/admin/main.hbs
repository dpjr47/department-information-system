  <script>
    $(document).ready(function(){
      $("#register").keyup(function(){
        var register_no = $(this).val().trim();
        if(register_no != ''){
          $.ajax({
            url: '/admin/studentCheck',
            type: 'post',
            data: {register_no: register_no},
            success: function(response){
                $('#std_response').html(response);
              }
            });
          }else{
            $("#std_response").html("");
          }
      });

      $("#tchr_pen").keyup(function(){
        var pen = $(this).val().trim();
        if(pen != ''){
          $.ajax({
            url: '/admin/teacherCheck',
            type: 'post',
            data: {pen: pen},
            success: function(response){
                $('#tchr_response').html(response);
              }
            });
          }else{
            $("#tchr_response").html("");
          }
      });

      $("#admin_pen").keyup(function(){
        var pen = $(this).val().trim();
        if(pen != ''){
          $.ajax({
            url: '/admin/adminCheck',
            type: 'post',
            data: {pen: pen},
            success: function(response){
                $('#admin_response').html(response);
              }
            });
          }else{
            $("#admin_response").html("");
          }
      });

      $("#tchrselect").change(function(){
        var pen = $(this).val().trim();
        if(pen!=''){
          $.ajax({
            url : '/admin/getTeacher',
            type: 'get',
            data: {pen : pen},
            success: function(response){
              document.getElementById("tname").value = response.name;
              document.getElementById("tpen").value = response.PEN_ID;
              document.getElementById("tdob").value = response.DOB
              document.getElementById("tdoj").value = response.year_of_joining
              document.getElementById("tgender").value = response.gender
            }
          });
        }
      })

      $("#stdselect").change(function(){
        var reg = $(this).val().trim();
        if(reg!=''){
          $.ajax({
            url : '/admin/getStudent',
            type: 'get',
            data: {reg : reg},
            success: function(response){
              document.getElementById("sname").value = response.name;
              document.getElementById("sreg").value = response.register_no;
              document.getElementById("sdob").value = response.DOB;
              document.getElementById("sdoa").value = response.year_of_joining;
              document.getElementById("sgender").value = response.gender;
              document.getElementById("sfather").value = response.father_name;
              document.getElementById("smother").value = response.mother_name;
            }
          });
        }
      })
      datePickerId.max = new Date().toISOString().split("T")[0]
    });
  </script>

