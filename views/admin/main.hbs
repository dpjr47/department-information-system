                    <input type="text" placeholder="name" name="name" required value={{user.name}}>
                  </div>
                  <div class="input-box">
                    <span class="details">Date of Birth</span>
                    <input type="date" name="dob"  required value={{user.DOB}} >
                  </div>
                  <div class="input-box">
                    <span class="details">Email</span>
                    <input type="email" placeholder="email" name="email" value={{user.email}} required>
                  </div>
                  <div class="input-box">
                    <span class="details">Phone Number</span>
                    <input type="text" placeholder="number" name="phone" required value={{user.phone}}>
                  </div>
                  <div class="input-box">
                    <span class="details">Address</span>
                    <input type="text" placeholder="address" name="address" required value={{user.address}}>
                  </div>
                  <div class="input-box">
                    <span class="details">City</span>
                    <input type="text" placeholder="City" name="city" required value={{user.post}}>
                  </div>
                  <div class="input-box">
                    <span class="details">District</span>
                    <input type="text" placeholder="District" name="district" required value={{user.district}}>
                  </div>
                  <div class="input-box">
                    <span class="details">State/UT</span>
                    <input type="text" placeholder="State/Union Territory" name="state" required value={{user.state}}>
                  </div>
                  <div class="input-box">
                    <span class="details">Pincode</span>
                    <input type="text" placeholder="Pincode" name="pincode" required value={{user.pincode}}>
                  </div> 
                  <div class="input-box">
                    <span class="details">Date of Joining</span>
                    <input type="date" placeholder="Date of Joining" name="date_of_joining" required value={{user.date_of_joining}}>
                  </div>
                  <div class="input-box">
                    <span class="details">College</span>
                    <input type="text" placeholder="School of Engineering" disabled>
                  </div>
                  <div class="input-box">
                    <span class="details">Division</span>
                    <input type="text" placeholder="Computer Science and Engineering" disabled>
                  </div>     
                </div>
                <div class="button">
                  <input type="submit" value="Update">
                </div>
              </form>
            </div>
          </div>
     <!-- clickdiv3 -->

     <!-- clickdiv4 -->
    <div class="addbox" id="four">
      <div class="title">List of Admins</div>
